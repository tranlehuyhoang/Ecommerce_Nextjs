generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model user {
  id       Int     @id @default(autoincrement())
  name     String? @db.VarChar(255)
  email    String  @unique(map: "User_email_key") @db.VarChar(255)
  password String  @db.VarChar(255)
}
